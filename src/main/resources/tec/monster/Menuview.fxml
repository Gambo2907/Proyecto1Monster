<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="800.0" prefWidth="800.0"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="tec.monster.Controllers.Menucontroller">
   <children>
      <ImageView fitHeight="800.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Viewimages/fprincipal.png" />
         </image>
      </ImageView>
      <Button fx:id="banfitrion" layoutX="28.0" layoutY="82.0" mnemonicParsing="false" onAction="#ClickAnfitrion" prefHeight="49.0" prefWidth="207.0" style="-fx-background-color: #0ACF7A; -fx-background-radius: 80; -fx-text-fill: white;" text="ANFITRION" />
      <Button fx:id="binvitado" layoutX="28.0" layoutY="160.0" mnemonicParsing="false" onAction="#ClickInvitado" prefHeight="49.0" prefWidth="207.0" style="-fx-background-color: #0ACF7A; -fx-background-radius: 80; -fx-text-fill: white;" text="INVITADO" />
   </children>
</AnchorPane>
